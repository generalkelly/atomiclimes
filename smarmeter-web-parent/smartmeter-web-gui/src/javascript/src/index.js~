import _ from 'lodash';

var datetime;
var date="1988-01-03";
var time;
var timezone="+01:00";

function component() {
  let element = document.createElement('div');

  time="15:45:00";
  datetime=date+'T'+time+timezone;
  
  element.innerHTML = _.join(['Hello', datetime], ' ');


  
  
  
  
  return element;
}

document.body.appendChild(component());
